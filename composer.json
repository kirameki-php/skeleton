{
  "name": "kirameki/skeleton",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "../framework",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "ext-apcu": "*",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "ext-posix": "*",
    "kirameki/framework": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0"
  },
  "autoload": {
    "psr-4": {
      "Kirameki\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "discard-changes": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
